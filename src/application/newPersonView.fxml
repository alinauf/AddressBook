            <Button fx:id="cancelButton" layoutX="305.0" layoutY="241.0" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
            <Button fx:id="okButton" layoutX="412.0" layoutY="241.0" mnemonicParsing="false" onAction="#handleOk" text="Ok" />
         </children>
      </HBox>
      <Label layoutX="56.0" layoutY="14.0" text="New Person">
